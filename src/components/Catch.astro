<script>
  import { gsap } from 'gsap';

  const catchElements = document.querySelectorAll('.c-catch');
  catchElements.forEach((catchElement) => {
    if(!catchElement.classList.contains('is-loading')) return;
    const face = catchElement.querySelector('.face');
    const subject = catchElement.querySelector('.subject');
    const text = subject?.textContent;
    if (subject) {
      subject.textContent = '';
    }
    if (subject) {
      (subject as HTMLElement).style.visibility = 'visible';
    }
    let index = 0;

    setTimeout(() => {
      gsap.to(face, (
        {
          duration: .6,
          y: 0,
        }
      ))
    }, 800);

    function type() {
      if (index < (text?.length ?? 0)) {
        if (subject) {
          subject.textContent += text?.charAt(index) ?? '';
          if (subject?.textContent) {
            (subject as HTMLElement).style.maxWidth = `${subject.textContent.length}ch`;
          }
        }
        index++;
        setTimeout(type, 100); // 100ミリ秒ごとに次の文字を追加
      } else {
        subject?.classList.add('no-cursor');
      }

      console.log('hoge')
    }

    setTimeout(type, 1400); // アニメーションを開始
  })
</script>

